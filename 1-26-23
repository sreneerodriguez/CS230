//determine if n is odd
public static boolean isOdd(int n){
  if (n%2 == 1){
    return true;
  } return false;
}

public static boolean isOdd(int n){
  if (n%2 == 0){
    return false;
  } return true;
}

public static boolean isOdd(int n){
  return (n%2 == 1);
}

public static boolean isOdd(int n){
  return (n%2 != 0);
}

public static boolean isOdd(int n){
  return (n%2 != 0);
}



//determine if n is even
public static boolean isEven(int n){
  return (n%2==0);
}


//determine if num is divisible by factor
 public static boolean isDivisibleBy(int num, int factor){
 		return (num%factor == 0);
 }
 
 
 //determine in num is between and lo and hi
 public static boolean isBetween(double num, double lo, double hi){
 		return (num < hi && num > lo);
 }
 
