//coppy the contents of arr1 into arr2 knowing they have the same length
arr1;
arr2;
for(int i = 0; i < arr1.length; i++){
  arr2[i] = arr1[1];
  System.out.println(arr2[i]);
}


//computeAverage
public static double computeAverage(int[] data){
  int sum = 0;
  for(int i = 0; i < data.length; i++){
    sum+= data[i];
  }
  return sum/data.length;
}

//computeMaximum
public static int computeMaximum(int[] data){
  int maxVal;
  for(int i = 0; i < data.length - 1; i++){
     maxVal = Math.max(data[i], data[i+1]);
  }
  return maxVal
}
